    { "key": "ctrl+shift+space", "command": "workbench.action.terminal.focus"},
    { "key": "ctrl+shift+space", "command": "workbench.action.focusActiveEditorGroup", "when": "terminalFocus"},

    {
        "key": "ctrl+shift+z",
        "command": "workbench.action.terminal.toggleTerminal",
